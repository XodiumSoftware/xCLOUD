{
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "json.schemas": [
    {
      "fileMatch": ["*.json", "*.jsonc"],
      "url": "https://json.schemastore.org/package"
    }
  ],
  "rust-analyzer.linkedProjects": ["Cargo.toml"]
}
